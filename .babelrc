{
  "presets": [
    "react",
    "stage-2",
    ["env", {
			"targets": {
				"browsers": "last 2 versions"
			},
			"loose": true,
			"modules": false
		}]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "server": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        [ "babel-plugin-webpack-alias", { "config": "./webpack.config.test.js" } ]
      ],

    }
  }
}
